<?xml version="1.0"?>
<project name="Talend Component Kit Connectors"
         xmlns="http://maven.apache.org/DECORATION/1.0.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="
          http://maven.apache.org/DECORATION/1.6.0
          http://maven.apache.org/xsd/decoration-1.6.0.xsd">
  <skin>
    <groupId>org.apache.maven.skins</groupId>
    <artifactId>maven-fluido-skin</artifactId>
    <version>1.7</version>
  </skin>
  <custom>
    <fluidoSkin>
      <sideBarEnabled>true</sideBarEnabled>
      <topBarEnabled>false</topBarEnabled>
      <navBarStyle>navbar-inverse</navBarStyle>
      <copyrightClass>pull-right</copyrightClass>
      <breadcrumbDivider>Â»</breadcrumbDivider>
