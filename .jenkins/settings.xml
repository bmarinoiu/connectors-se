<?xml version="1.0"?>
<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="
            http://maven.apache.org/SETTINGS/1.0.0
            https://maven.apache.org/xsd/settings-1.0.0.xsd">
  <servers>
    <server>
      <id>talend.oss.snapshots</id>
      <username>${env.NEXUS_USER}</username>
      <password>${env.NEXUS_PASSWORD}</password>
      <configuration>
        <timeout>30000</timeout>
      </configuration>
    </server>
    <server>
      <id>talend.oss.releases</id>
      <username>${env.NEXUS_USER}</username>
      <password>${env.NEXUS_PASSWORD}</password>
      <configuration>
        <timeout>30000</timeout>
      </configuration>
    </server>
    <server>
      <id>talend_nexus_deployment</id>
      <username>${env.NEXUS_USER}</username>
      <password>${env.NEXUS_PASSWORD}</password>
      <configuration>
        <timeout>30000</timeout>
      </configuration>
    </server>
    <server>
      <id>onedrive</id>
      <username>${env.ONEDRIVE_INTEGRATION_USER}</username>
      <password>${env.ONEDRIVE_INTEGRATION_PASSWORD}</password>
    </server>
  </servers>

  <mirrors>
    <mirror>
      <id>maven_central_proxy</id>
      <name>Maven Central Proxy</name>
      <url>https://artifacts-zl.talend.com/nexus/content/repositories/central/</url>
      <mirrorOf>central</mirrorOf>
    </mirror>
  </mirrors>
</settings>
